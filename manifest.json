{
  "manifest_version": 3,
  "name": "OSS错误码助手",
  "version": "1.0",
  "description": "自动识别并解析阿里云OSS错误码，提供错误描述和快速跳转功能",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://help.aliyun.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["styles/tooltip.css"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "192": "icons/192.png"
  }
} 